{
  "name": "gulp-hmin",
  "version": "0.0.3",
  "description": "Gulp plugin for minify html files",
  "keywords": [
    "gulp",
    "minify",
    "html"
  ],
  "license": "MIT",
  "author": "llcawc",
  "homepage": "https://github.com/llcawc/gulp-hmin",
  "type": "module",
  "main": "./lib/gulp-hmin.js",
  "exports": "./lib/gulp-hmin.js",
