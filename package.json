    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "prettier-plugin-embed": "^0.5.0",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vinyl": "^3.0.1"
